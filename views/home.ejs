<!DOCTYPE html>
<html>

<head>
  <title>Dev</title>
  <link rel="stylesheet" type="text/css" href='/stylesheets/style.css' />
</head>

<body>
  <table class="user-table">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Avatar</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(function (user) { %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.avatar %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
  <table class="user-table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Commisions</th>
        <th>Sales</th>
        <th>Leads</th>
        <th>Clicks</th>
        <th>EPC</th>
        <th>Impressions</th>
        <th>CR</th>
      </tr>
    </thead>
    <tbody>
      <% dates.forEach(function (date) { %>
      <tr>
        <td><%= date.date %></td>
        <td><%= date.commisions %></td>
        <td><%= date.sales %></td>
        <td><%= date.leads %></td>
        <td><%= date.clicks %></td>
        <td><%= date.epc %></td>
        <td><%= date.impressions %></td>
        <td><%= date.cr %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</body>

</html>