<!DOCTYPE html>
<html>
  <head>
    <title>Dev</title>
    <link rel="stylesheet" type="text/css" href='/stylesheets/style.css' />
  </head>
  <body>
    <table class="user-table" >  
      <thead>  
          <tr>  
              <th>First Name</th>  
              <th>Last Name</th>  
              <th>Email</th>  
              <th>Avatar</th>  
          </tr>  
      </thead>  
       <tbody>  
       <% users.forEach(function (user) { %>
          <tr>  
              <td><%= user.first_name %></td>  
              <td><%= user.last_name %></td>  
              <td><%= user.email %></td>  
              <td><%= user.avatar %></td>
          </tr>                                   
       <% }) %>
       </tbody>
  </table>   
  </body>
</html>